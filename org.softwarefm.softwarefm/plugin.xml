      <view
            name="Debug - Text"
            icon="icons/debug.png"
            category="org.softwarefm"
            class="org.softwarefm.softwarefm.views.SoftwareFmDebugView"
            id="org.softwarefm.softwarefm.views.SoftwareFmDebugView">
