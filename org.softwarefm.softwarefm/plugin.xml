      <view
            name="Code"
            icon="icons/class.png"
            category="org.softwarefm"
            class="org.softwarefm.softwarefm.views.ClassAndMethodView"
            id="org.softwarefm.softwarefm.views.ClassAndMethodView">
      </view>

      <view
            name="Artifact"
            icon="icons/project.png"
            category="org.softwarefm"
